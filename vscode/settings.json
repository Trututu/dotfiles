{
  // looks
  "workbench.colorCustomizations": {},
  // editor
  "editor.renderLineHighlight": "line",
  "editor.hideCursorInOverviewRuler": true,
  "editor.scrollBeyondLastLine": false,
  "editor.matchBrackets": false,
  "editor.occurrencesHighlight": false,
  "editor.multiCursorModifier": "alt",
  "editor.overviewRulerBorder": false,
  "editor.overviewRulerLanes": 0,
  "editor.renderIndentGuides": true,
  "editor.folding": false,
  "editor.snippetSuggestions": "top",
  "editor.lightbulb.enabled": false,
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "solid",
  "editor.tabSize": 2,
  "editor.lineNumbers": "relative",
  "editor.cursorStyle": "line",
  // workbench
  "workbench.list.horizontalScrolling": true,
  "workbench.statusBar.feedback.visible": false,
  "workbench.fontAliasing": "antialiased",
  "workbench.startupEditor": "none",
  "workbench.settings.editor": "json",
  "workbench.editor.showIcons": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  // window
  "window.zoomLevel": 1,
  "window.restoreWindows": "all",
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "window.newWindowDimensions": "maximized",
  // debug
  "debug.openDebug": "neverOpen",
  "debug.showInStatusBar": "never",
  "debug.toolBarLocation": "hidden",
  // fonts
  "editor.fontFamily": "Fira Code",
  "terminal.integrated.fontSize": 15,
  "editor.fontLigatures": false,
  "editor.fontSize": 15,
  // new file
  // "newFile.defaultFileExtension": ".go",
  // "newFile.showPathRelativeTo": "project",
  // search
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "search.smartCase": true,
  "search.exclude": {
    "dist": true,
    "out": true,
    "coverage": true
  },
  // terminal
  "terminal.integrated.copyOnSelection": true,
  // files
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "vendor": true,
    "package-lock.json": true,
    "**/node_modules": true,
    "**/npm-debug.log": true,
    "**/*.bs.js": true
  },
  // explorer
  "explorer.openEditors.visible": 0,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": false,
  // prettier
  "editor.formatOnSave": true,
  // code runner
  // sync settings
  // "sync.gist": "94288845f0956f08d4766b477001c9eb",
  // "sync.autoDownload": false,
  // "sync.autoUpload": false,
  // "sync.forceDownload": false,
  // "sync.quietSync": false,
  // "sync.askGistName": false,
  // viwm
  "vim.leader": ",",
  "vim.cursorStylePerMode.normal": "block",
  "vim.useSystemClipboard": true,
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "red",
  // space map
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "w"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "q"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "a"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        },
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "leader",
        "leader",
        "k"
      ]
    },
    {
      "before": [
        "J"
      ],
      "after": [
        "leader",
        "leader",
        "j"
      ]
    },
    {
      "before": [
        "<leader>",
        "i"
      ],
      "after": [
        "y",
        "i",
        "\""
      ]
    },
    {
      "before": [
        "Y"
      ],
      "after": [
        "y",
        "$"
      ]
    },
    {
      "before": [
        "<leader>",
        "c"
      ],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    }
  ],
  // files
  "files.associations": {
    "*.toml": "toml"
  },
  "explorer.confirmDelete": false,
  // latex
  // typescript
  "typescript.check.npmIsInstalled": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  // javascript
  "javascript.updateImportsOnFileMove.enabled": "never",
  // git
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.ignoreLimitWarning": true,
  "git.enableSmartCommit": true,
  "git.ignoreMissingGitWarning": true,
  // gitlens
  "gitlens.codeLens.enabled": false,
  "gitlens.settings.mode": "advanced",
  "gitlens.keymap": "chorded",
  "gitlens.codeLens.scopes": [
    "containers",
    "document"
  ],
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": true,
    "suppressWelcomeNotice": false
  },
  "colorHelper.disableGpu": 0,
  // extensions
  "extensions.ignoreRecommendations": true,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "window.titleBarStyle": "native",
  "window.openFilesInNewWindow": "off",
  "workbench.iconTheme": "material-icon-theme",
  "emmet.triggerExpansionOnTab": true,
  "html.autoClosingTags": true,
  "terminal.integrated.rendererType": "auto",
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "gitlens.mode.active": "zen",
  "workbench.sideBar.location": "right",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "vim.camelCaseMotion.enable": true,
  "vim.foldfix": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.enableNeovim": true,
  "search.showLineNumbers": true,
  "editor.parameterHints.cycle": true,
  "dart.flutterHotRestartOnSave": true,
  "workbench.activityBar.visible": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
